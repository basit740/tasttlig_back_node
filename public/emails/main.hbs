<html lang="en">

<head>
  <link rel="stylesheet" href="./css/email_main.css">
</head>

<body>
  <table cellpadding="0" cellspacing="0" width="100%"
    style="width: 100%; border: 0px; font-family: Arial, Helvetica, sans-serif; color: rgb(17, 17, 17); font-size: 13px; border-collapse: collapse; letter-spacing: normal; text-rendering: optimizeLegibility; background-color: rgb(240, 240, 240)">
    <tbody>
      <tr>
        <td align="center" style="border: 0px; padding-top: 10px; padding-bottom: 20px">
          <br>
          <table cellpadding="0" cellspacing="0" width="600"
            style="min-width: 600px; max-width: 600px; width: 600px; border: 0px; border-collapse: collapse; table-layout: fixed; background-color: rgb(255, 255, 255)">
            <tbody>
              <tr>
                <td valign="top" width="600"
                  style="width: 600px; border: 0px; min-height: 570px; word-wrap: break-word; word-break: break-word">
                  <table cellspacing="0" cellpadding="0"
                    style="min-width: 100%; max-width: 100%; table-layout: fixed; border: 0px; border-collapse: collapse"
                    width="100%">
                    <tbody>
                      {{> header }}
                    <tbody>
                      <tr>
                        <td
                          style="word-wrap: break-word; word-break: break-word; border: 0px; padding: 30px 30px; border-top: 0px none rgb(17, 17, 17); border-bottom: 0px none rgb(17, 17, 17); font-family: Arial, Helvetica, sans-serif">
                          <div style="margin: 0px">
                            <div style="margin: 0px">
                              {{{body}}}
                            </div>
                          </div>
                        </td>
                      </tr>
                    </tbody>
                    {{> footer }}
                  </table>
                </td>
              </tr>
            </tbody>
          </table>
        </td>
      </tr>
    </tbody>
  </table>
</body>

</html>